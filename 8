SELECT 
  ts.ENDPOINT_BACKEND,
  ts.APP_NAME,
  CASE 
    WHEN ts.STATUS = 'PASS' THEN 'UP'
    ELSE 'DOWN'
  END AS STATUS_INDICATOR
FROM cqeexecution.teststeps ts
WHERE ts.ENDPOINT_BACKEND IS NOT NULL
GROUP BY ts.ENDPOINT_BACKEND, ts.APP_NAME, STATUS_INDICATOR
ORDER BY ts.ENDPOINT_BACKEND, ts.APP_NAME;
